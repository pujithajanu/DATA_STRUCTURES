#include <stdio.h>

int main() {
    int max,min,n;
    printf("enter n:");
    scanf("%d",&n);
    int a[n];
    printf("enter array elements:\n");
    for(int i=0;i<n;i++)
    scanf("%d",&a[i]);
    min=a[0];
    max=a[0];
    for(int i=0;i<n;i++)
    {
        if(min>a[i])
        min=a[i];
        if(max<a[i])
        max=a[i];
        
    }
    printf("min:%d,max:%d",min,max);
    return 0;
}
